 <!--<mapper namespace="org.zerock.mapper.MemberMapper">  -->
 <mapper namespace="member">
 	<select id="getTime" resultType="string">
 	select now()
 	<!-- select sysdate() from dual --><!-- oracle 인경우 -->
 	</select>
 	
 	<insert id="insertMember">
	INSERT INTO TBL_MEMBER 
		 	(USERID 
		 	, USERPW 
		 	, USERNAME
		 	, EMAIL)
 	 	VALUES
		 	( #{userid}
		 	, #{userpw}
		 	, #{username}
		 	, #{email})
 	</insert>
 </mapper>
  
  